<div class="weekly">
	<h3>{{date 'MMM D' weekStart}} - {{date 'MMM D' weekEnd}}</h3>
	<table class="table table-striped weekly-transactions">
		<thead>
			<tr>
				<th>Day</th>
				<th>Merchant</th>
				<th>Amount</th>
				<th>Source</th>
				<th class="secondary">Desc.</th>
				<th class="secondary">Category</th>
				<th class="secondary"></th>
			</tr>
			<tr class="addition">
				<th class="two-col">Desc.</th>
				<th>Category</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
	<div class="summary">
		<div class="subtotal"><span>Week Total</span> <span class="value">{{money weekTotal}}</span></div>
	</div>
</div>
