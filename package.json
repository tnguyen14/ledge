{
  "name": "ledge",
  "version": "3.0.0",
  "description": "ledge. in react with redux.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:tnguyen14/ledge.git"
  },
  "scripts": {
    "serve": "http-server dist -p 14001",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "cp index.html dist/ && cp manifest.json dist/",
    "font": "mkdir dist/fonts && find node_modules/bootstrap-sass/assets/fonts/bootstrap -iname 'glyphicon*' -exec cp \\{\\} dist/fonts/ \\;",
    "assets": "cp assets/*.{svg,png} dist/",
    "css": "node-sass scss/style.scss dist/style.css",
    "css:watch": "node-sass -w scss/style.scss dist/style.css",
    "js": "browserify index.js -t babelify -o dist/index.js",
    "js:watch": "watchify index.js -t babelify -o dist/index.js -dv",
    "static": "npm run copy && npm run font && npm run assets",
    "build": "npm run clean && npm run static && npm run css && npm run js",
    "watch": "npm run js:watch & npm run css:watch & npm run serve",
    "start": "npm run build && npm run watch",
    "deploy": "npm run build && gh-pages -d dist",
    "precommit": "lint-staged"
  },
  "browser": {
    "config": "./config.json"
  },
  "keywords": [
    "ledge"
  ],
  "author": "Tri Nguyen <tri@tridnguyen.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tnguyen14/ledge/issues"
  },
  "homepage": "https://github.com/tnguyen14/ledge",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint-config-happiness": "^5.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-react": "^7.1.0",
    "gh-pages": "^0.8.0",
    "http-server": "^0.9.0",
    "husky": "^0.14.2",
    "lint-staged": "^4.0.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.5.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.6",
    "isomorphic-fetch": "^2.2.1",
    "moment-timezone": "^0.5.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.2",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "simple-fetch": "^1.2.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --use-tabs --single-quote",
      "git add"
    ]
  }
}
