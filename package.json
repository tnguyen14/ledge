{
  "name": "ledge",
  "version": "1.0.0",
  "description": "ledge. in react.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:tnguyen14/ledge.git"
  },
  "scripts": {
    "serve": "http-server dist -p 4001",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "cp index.html dist/ && cp manifest.json dist/",
    "css": "node-sass scss/style.scss | postcss -u autoprefixer -o dist/style.css",
    "font": "mkdir dist/fonts && find node_modules/bootstrap-sass/assets/fonts/bootstrap -iname 'glyphicon*' -exec cp \\{\\} dist/fonts/ \\;",
    "css:watch": "node-sass -w scss/style.scss dist/style.css & postcss -u autoprefixer -o dist/style.css dist/style.css",
    "js": "browserify index.js -t babelify -o dist/index.js",
    "js:watch": "watchify index.js -t babelify -t [ envify --NODE_ENV development ] -o dist/index.js -dv",
    "build": "npm run clean && npm run copy && npm run font && npm run css && npm run js",
    "start": "npm run build && npm run js:watch & npm run css:watch & npm run serve",
    "gh-pages": "./bin/gh-pages.js",
    "deploy": "npm run build && npm run gh-pages"
  },
  "browser": {
    "config": "./config.json"
  },
  "keywords": [
    "ledge"
  ],
  "author": "Tri Nguyen <tri@tridnguyen.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tnguyen14/ledge/issues"
  },
  "homepage": "https://github.com/tnguyen14/ledge",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "gh-pages": "^0.8.0",
    "http-server": "^0.9.0",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.4.0",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.3",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.0",
    "moment-timezone": "^0.5.0",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-form": "^4.1.3",
    "redux-logger": "^2.3.2",
    "redux-thunk": "^1.0.3",
    "restify": "^4.0.3",
    "serve-static": "^1.10.1",
    "simple-fetch": "^1.2.0",
    "slug": "^0.9.1",
    "typeahead.js": "^0.11.1"
  }
}
